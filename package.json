    "cypress": "^12.10.0",
    "eslint": "^8.38.0",
    "sass": "^1.62.0"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "joi": "^17.9.1",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.10.0",
    "swagger-ui-express": "^4.6.2",
    "yaml": "^2.2.1"
  }
